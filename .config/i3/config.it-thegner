# Start i3bar to display a workspace bar (plus the system information i3status
# finds out, if available)
bar {
	tray_output primary
	output DVI-I-1
	status_command /home/thegner/go/src/github.com/travishegner/i3gobar/example/main
}

bar {
	tray_output none
	output DVI-D-0
	status_command /home/thegner/go/src/github.com/travishegner/i3gobar/example/left/left
}

bar {
	tray_output none
	output HDMI-0
	status_command /home/thegner/go/src/github.com/travishegner/i3gobar/example/right/right
}

